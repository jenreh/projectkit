{
    "servers": {
        "memory": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ]
        },
        "code-reasoning": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@mettamatt/code-reasoning"
            ]
        },
        "duckduckgo": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "duckduckgo-mcp-server"
            ]
        },
    }
}
